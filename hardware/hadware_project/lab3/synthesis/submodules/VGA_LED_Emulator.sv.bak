	initial begin
   for (i=0; i<17;i++)
   inScreen[i] <= ((vcount == i)&(hcount[10:1]<=screen[i][19:10])&(hcount[10:1]>=screen[i][9:0]));
   
   end
	
	integer j;
	initial begin
   for (j=0; j<17;j++)
	color[j] <= ((screen[j][20]==1));
   end
	*/
	
